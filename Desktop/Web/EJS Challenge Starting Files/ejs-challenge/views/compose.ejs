<%- include('partial/header'); -%>

<h1>Compose</h1>

<form action="/compose" method="post">
    <div class="mb-3">
      <label class="form-label">Title</label>
      <input name="postTitle" type="text" class="form-control" placeholder="Post title">
    </div>
    <div class="mb-3">
      <label class="form-label">Post</label>
      <textarea name="postBody" type="textarea" class="form-control" rows="5" placeholder="Descriptions"></textarea>
    </div>
    <button name="button" type="submit" class="btn btn-primary" id="btn-publish">Publish</button>
    
  </form>

<%- include('partial/footer'); -%>